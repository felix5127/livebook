{
  "name": "livebook-mvp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "analyze": "ANALYZE=true npm run build",
    "test": "echo \"Warning: No tests specified\" && exit 0",
    "preview": "npm run build && npm run start",
    "clean": "rm -rf .next out dist",
    "db:generate": "supabase gen types typescript --project-id=your-project-id --schema=public > types/database.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "env:validate": "node scripts/test-env-validation.js",
    "predev": "echo 'Starting development server...'",
    "prebuild": "npm run type-check",
    "postbuild": "echo 'Build completed successfully!'",
    "vercel-build": "npm run build"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.7",
    "@heroicons/react": "^2.2.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.45.0",
    "axios": "^1.11.0",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "framer-motion": "^12.23.12",
    "jose": "^6.0.12",
    "lucide-react": "^0.400.0",
    "next": "^14.2.0",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-hot-toast": "^2.6.0",
    "tailwind-merge": "^2.6.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@swc/core": "^1.13.3",
    "@types/crypto-js": "^4.2.2",
    "@types/node": "^20.14.0",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.0",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.0"
  }
}
